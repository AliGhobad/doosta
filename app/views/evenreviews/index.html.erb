<p id="notice"><%= notice %></p>

<h1>Evenreviews</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>User</th>
      <th>Scores</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evenreviews.each do |evenreview| %>
      <tr>
        <td><%= evenreview.event %></td>
        <td><%= evenreview.user %></td>
        <td><%= evenreview.scores %></td>
        <td><%= evenreview.comments %></td>
        <td><%= link_to 'Show', evenreview %></td>
        <td><%= link_to 'Edit', edit_evenreview_path(evenreview) %></td>
        <td><%= link_to 'Destroy', evenreview, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evenreview', new_evenreview_path %>
