<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Event name</th>
      <th>Address</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Description</th>
      <th>Photos</th>
      <th>Links to share</th>
      <th>Fees</th>
      <th>Available</th>
      <th>Capacity</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.event_name %></td>
        <td><%= event.address %></td>
        <td><%= event.start_time %></td>
        <td><%= event.end_time %></td>
        <td><%= event.description %></td>
        <td><%= event.photos %></td>
        <td><%= event.links_to_share %></td>
        <td><%= event.fees %></td>
        <td><%= event.available %></td>
        <td><%= event.capacity %></td>
        <td><%= event.user.inspect %></td>
        <td><%= event.userlog.inspect %></td>
        <td><%= event.category %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
